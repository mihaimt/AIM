grid
{
   read_from_file   no 
   //file_name      mesh.dat
   x_min   0
   x_max   1
   n_cell  100
}

numeric
{
   time_scheme  ssprk3
   cfl          0.8
   max_iter     50000
   final_time   1
   min_residue  1.0e-6
   reconstruct  first  
   bc_type      neumann
}

material
{
   gamma       1.4
   gas_const   1.0
   viscosity
   {
      model    constant 
      mu_ref   1e-3
   }
   prandtl     0.72
   model       ns
   flux        roe
}

constants
{
   d    1.0
   u    1.0
   p    1.0
}

initial_condition
{
   read_from_file   no
   //file_name    ****
   density          d
   velocity         u
   pressure         p
}

output
{
   frequency 1
}
